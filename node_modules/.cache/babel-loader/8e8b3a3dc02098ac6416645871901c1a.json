     }\n            })\n            .catch(data => {\n                console.log(data);\n            });\n      }\n    }\n  },\n  methods: {\n    onInput(key) {\n      this.value = (this.value + key).slice(0, 6);\n    },\n    onDelete() {\n      this.value = this.value.slice(0, this.value.length - 1);\n    }\n  },\n  components: { Headers }\n};\n</script>\n\n<style lang=\"less\">\n.qz_setting,\n.qz_txt {\n  text-align: center;\n}\n.qz_setting {\n  font-size: 17px;\n  margin-top: 40px;\n  margin-bottom: 15px;\n}\n.qz_txt {\n  margin-bottom: 30px;\n}\n</style>\n"],"sourceRoot":"src/views"}]}